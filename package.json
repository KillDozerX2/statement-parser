{
    "name": "statement-parser",
    "version": "2.0.0",
    "description": "Parse bank and credit card statements.",
    "keywords": [
        "pdf",
        "bank",
        "credit card",
        "finances",
        "finance",
        "financial",
        "pdf to json",
        "citi",
        "chase",
        "usaa",
        "costco",
        "prime",
        "paypal",
        "parser",
        "statement",
        "statements"
    ],
    "homepage": "https://github.com/electrovir/statement-parser",
    "bugs": {
        "url": "https://github.com/electrovir/statement-parser/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/electrovir/statement-parser"
    },
    "license": "MIT",
    "author": {
        "name": "electrovir",
        "url": "https://github.com/electrovir"
    },
    "main": "dist/src/index.js",
    "typings": "dist/src/index.d.ts",
    "scripts": {
        "compile": "rm -rf dist && tsc",
        "format:base": "prettier --ignore-path .gitignore \"./{,!(dist)/**/}*.+(ts|json|html|css)\"",
        "format:check": "npm run format:base -- --check",
        "format": "npm run format:base -- --write",
        "prepublishOnly": "npm install && npm run test && npm run spellcheck",
        "sanitize": "npm run compile && node dist/sanitizer/sanitize-for-test-file-cli.js",
        "sanitize:no-compile": "node dist/sanitizer/sanitize-for-test-file-cli.js",
        "sanitize:all": "./bulk-sanitize.sh files/downloads",
        "spellcheck": "cspell --no-progress --unique \"./**/*\"",
        "test:full": "npm run test && npm run spellcheck -- --no-progress && npm run format:check",
        "test": "npm run compile && test-vir \"./**/!(*.type).test.js\""
    },
    "dependencies": {
        "fs-extra": "^10.0.0",
        "pdf-text-reader": "^2.0.3",
        "pdfjs-dist": "^2.8.335"
    },
    "devDependencies": {
        "@types/fs-extra": "^9.0.12",
        "@types/node": "^16.4.1",
        "@types/pdfjs-dist": "^2.7.4",
        "@types/pdfkit": "^0.11.2",
        "cspell": "^5.6.6",
        "pdfkit": "^0.12.1",
        "prettier": "^2.3.2",
        "prettier-plugin-jsdoc": "^0.3.23",
        "prettier-plugin-organize-imports": "^2.3.3",
        "prettier-plugin-packagejson": "^2.2.11",
        "prettier-plugin-sort-json": "^0.0.2",
        "test-vir": "^0.2.0",
        "typescript": "^4.3.5"
    },
    "engines": {
        "node": ">=12",
        "npm": ">=7"
    }
}
